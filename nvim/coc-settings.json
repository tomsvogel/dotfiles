{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascript.jsx",
    "javascriptreact",
    "Markdown",
    "php"
  ],
  "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "typescript.tsx"],
  "css.filetypes": [
    "css",
    "less",
    "scss",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascript.jsx",
    "javascriptreact",
    "typescript.tsx"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "eslint.autoFixOnSave": false,
  "prettier.useTabs": false,
  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.trailingComma": "es5",
  "prettier.jsxSingleQuote": true,
  "prettier.bracketSpacing": false,
  "prettier.eslintIntegration": true,

  "languageserver": {
    "phplang": {
      "command": "php",
      "args": [
        "/Users/toms/.config/nvim/plugged/LanguageServer-php-neovim/vendor/felixfbecker/language-server/bin/php-language-server.php"
      ],
      "filetypes": ["php"]
    }
  },
  "snippets.ultisnips.directories": ["/Users/toms/.dotfiles/nvim/snips"],
  "snippets.extends": {
    "cpp": ["c"],
    "javascript.jsx": ["javascript"],
    "typescript": ["javascript"],
    "typescriptreact": ["javascript"]
  }
}
